#!/bin/bash
@Python3_EXECUTABLE@ "@CMAKE_CURRENT_SOURCE_DIR@/3rdparty/run-clang-tidy/run-clang-tidy.py" `find "@CMAKE_SOURCE_DIR@"/{src,tests} -type f -iname '*.cpp'` -p "@CMAKE_BINARY_DIR@" "--header-filter=`echo '@CMAKE_SOURCE_DIR@/(include|src|tests)/.*' | sed 's/\\//\\\\\//g'`" --clang-tidy-binary @CLANG_TIDY_PROGRAM@ "$@"